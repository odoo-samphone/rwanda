<odoo>
	<data>
		 <record id="base_res_partner_view_form" model="ir.ui.view">
        <field name="name">inherit.base.res.partner.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_payment_term_id']"
                   position="after">
                <field name="affiliated"/>
            </xpath>
        </field>
    </record>
	</data>

</odoo>